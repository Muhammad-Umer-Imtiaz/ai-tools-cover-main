{
  "name": "admin-dashboard",
  "version": "1.0.0",
  "description": "Combined website template with Payload CMS and Frontend",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test --config=playwright.config.ts",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@payloadcms/admin-bar": "3.50.0",
    "@payloadcms/db-mongodb": "3.50.0",
    "@payloadcms/live-preview-react": "3.50.0",
    "@payloadcms/next": "3.50.0",
    "@payloadcms/payload-cloud": "3.50.0",
    "@payloadcms/plugin-cloud-storage": "^3.55.1",
    "@payloadcms/plugin-form-builder": "3.50.0",
    "@payloadcms/plugin-nested-docs": "3.50.0",
    "@payloadcms/plugin-redirects": "3.50.0",
    "@payloadcms/plugin-search": "3.50.0",
    "@payloadcms/plugin-seo": "3.50.0",
    "@payloadcms/richtext-lexical": "3.50.0",
    "@payloadcms/ui": "3.50.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "bcrypt": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "cloudinary": "^2.7.0",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "csv-parse": "^6.1.0",
    "dotenv": "16.4.7",
    "formidable": "^3.5.4",
    "graphql": "^16.11.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.511.0",
    "multer": "^2.0.2",
    "next": "15.4.4",
    "next-sitemap": "^4.2.3",
    "papaparse": "^5.5.3",
    "payload": "3.50.0",
    "prism-react-renderer": "^2.4.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "7.45.4",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "sharp": "0.34.2",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^4.1.12",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@playwright/test": "1.54.1",
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/react": "16.3.0",
    "@types/escape-html": "^1.0.4",
    "@types/node": "22.5.4",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "autoprefixer": "^10.4.21",
    "css-loader": "^7.1.2",
    "eslint": "^9.34.0",
    "eslint-config-next": "15.4.4",
    "jsdom": "26.1.0",
    "playwright": "1.54.1",
    "playwright-core": "1.54.1",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^10.3.1",
    "prettier": "^3.6.2",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.91.0",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "typescript": "5.7.3",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  }
}